<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Parallel" thread-count="8" parallel="tests">

    <listeners>
        <listener class-name="util.ListenersReport"></listener>
    </listeners>

    <parameter name="device" value="browser"/>

    <test name="Test1">
        <parameter name="Caso" value="TC_Actualizar las tasas en la pantalla principal"></parameter>
        <parameter name="config" value="parallel.conf.json"></parameter>
        <parameter name="environment" value="Chrome"></parameter>
        <parameter name="row" value="1"></parameter>
        <classes>
            <class name="com.ldf.TestRates">
                <methods>
                    <parameter name="nameTestCase" value="TC_Actualizar las tasas en la pantalla principal"></parameter>
                    <include name=".*createRate"></include>
                </methods>
            </class>
        </classes>
    </test>

</suite>